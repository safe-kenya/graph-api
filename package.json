  "license": "MIT",
  "scripts": {
    "test": "export NODE_ENV=test && mocha --require babel-register --inspect test/**/*.js --exit",
    "dev": "nodemon --exec babel-node --presets babel-preset-env --inspect  ./src/server",
    "build": "babel ./src --out-dir dist --source-maps ",
    "deploy": "yarn build && gcloud functions deploy smartkids-graph --trigger-http --region=us-central1 --runtime=nodejs10 --entry-point=default --env-vars-file .env.yaml",
    "test-deployment": "yarn build && node dist/server",
    "start": "node ./dist/server",
    "debug": "babel-node debug ./src"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.3",
    "express-graphql": "^0.9.0",
    "graphql": "^14.4.2",
    "graphql-errors": "^2.1.0",
    "graphql-import": "^0.7.1",
    "graphql-import-node": "^0.0.4",
    "graphql-tools": "^4.0.5",
    "morgan": "^1.9.1",
    "rimraf": "^2.6.3",
    "sails-disk": "^1.1.2",
    "sails-mongo": "^1.0.1",
    "sails-postgresql": "^1.0.2",
    "source-map-support": "^0.5.13",
    "uuidv4": "^4.0.0",
    "waterline": "^0.13.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.4",
    "nyc": "^14.1.1"
  }
}
